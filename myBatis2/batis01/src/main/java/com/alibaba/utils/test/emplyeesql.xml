<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
        namespace: 名称空间，类似类的包名
        在mapper标签中，是什么操作，就编写什么标签

            查询： <select>
            删除： <delete>
            更新： <update>
            插入： <insert>
 -->
<mapper namespace="myemployee"><!-- 这里myemployeez是自定义的，都可以 -->

    <!--
            id； sql的唯一标识，写方法名
            resultType： sql返回值的类型，只有select标签才需要编写返回值类型

            #{xxx}: 类似于?，占位符
     -->
    <select id="selectEmp" resultType="com.atguigu.mybatis.pojo.Employee">
        select * from tbl_employee where id = #{xxx}
    </select>

</mapper>